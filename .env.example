# Snowflake Connection Parameters
# Copy this file to .env and fill in your values

# Required: Snowflake account identifier (without .snowflakecomputing.com)
SNOWFLAKE_ACCOUNT=your-account-identifier

# Required: Authentication
SNOWFLAKE_USER=your-username
SNOWFLAKE_PASSWORD=your-password
# OR use Personal Access Token
# SNOWFLAKE_PAT=your-personal-access-token

# Optional: Connection settings
SNOWFLAKE_ROLE=your-default-role
SNOWFLAKE_WAREHOUSE=your-default-warehouse
SNOWFLAKE_HOST=your-account.snowflakecomputing.com

# Optional: Multi-factor authentication
# SNOWFLAKE_PASSCODE=your-mfa-code
# SNOWFLAKE_PASSCODE_IN_PASSWORD=false

# Optional: Key pair authentication
# SNOWFLAKE_PRIVATE_KEY=your-private-key
# SNOWFLAKE_PRIVATE_KEY_FILE=/path/to/private_key.pem
# SNOWFLAKE_PRIVATE_KEY_PWD=private-key-passphrase

# Optional: SSO/SAML authentication via web browser
# SNOWFLAKE_AUTHENTICATOR=externalbrowser
# Note: Browser will open for authentication. User and account are required, password is not needed.

# Optional: OAuth authentication  
# SNOWFLAKE_AUTHENTICATOR=oauth

# Optional: Native Okta authentication
# SNOWFLAKE_AUTHENTICATOR=https://your-company.okta.com

# Required: Service configuration file
SERVICE_CONFIG_FILE=configs/minimal.yaml